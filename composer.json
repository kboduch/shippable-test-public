{
    "name": "ibodzio/shippable-test-public",
    "license": "MIT",
    "require-dev": {
        "behat/behat": "^3.5",
        "leanphp/phpspec-code-coverage": "^4.2",
        "phpspec/phpspec": "^4.2",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Krzysztof Boduch",
            "email": "boduch.krzysztof@gmail.com"
        }
    ]
}
